    "type": "git",
    "url": "git+https://github.com/DemoMacro/nuxt-starter.git"
  },
  "author": {
    "name": "Demo Macro",
    "email": "abc@imst.xyz",
    "url": "https://imst.xyz/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DemoMacro/nuxt-starter/issues"
  },
  "homepage": "https://github.com/DemoMacro/nuxt-starter#readme",
  "devDependencies": {
    "@funish/githooks": "edge",
    "@funish/githooks-config": "edge",
    "@funish/lint": "edge",
    "nuxt": "3.6.2",
    "prettier": "3.0.0",
    "rome": "12.1.3"
  }
}
