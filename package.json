{
  "name": "nuxt-starter",
  "version": "0.0.0",
  "description": "Starter for Nuxt, powered by Demo Macro.",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "check": "biome check . --apply-unsafe && biome format . --write",
    "format": "prettier --write --list-different . --ignore-path .gitignore",
    "postinstall": "nuxt prepare && githooks install"
  },
