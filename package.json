    "url": "https://imst.xyz/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DemoMacro/nuxt-starter/issues"
  },
  "homepage": "https://github.com/DemoMacro/nuxt-starter#readme",
  "devDependencies": {
    "@biomejs/biome": "1.7.1",
    "@funish/githooks": "0.0.5",
    "@funish/githooks-config": "0.0.2",
    "@funish/lint": "0.0.2",
    "nuxt": "3.11.2",
    "prettier": "3.2.5"
  }
